input {
  redis {
    host => "127.0.0.1"
    type => "redis-input"
    # these settings should match the output of the agent
    data_type => "list"
    key => "logstash"

    # We use json_event here since the sender is a logstash agent
    format => "json_event"
    password => "<%= scope.lookupvar("::logstash::indexer::redis_password") %>"
  }
}

output {
  elasticsearch {
    host => "127.0.0.1"
  }
}
