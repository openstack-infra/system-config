[access "refs/heads/*"]
        label-Code-Review = -2..+2 group sahara-guestagent-core
        label-Workflow = -1..+1 group sahara-guestagent-core
[access "refs/heads/stable/*"]
        exclusiveGroupPermissions = label-Code-Review label-Workflow
        label-Code-Review = -2..+2 group sahara-guestagent-core
        label-Workflow = -1..+1 group sahara-guestagent-core
        label-Code-Review = -1..+1 group Registered Users
[access "refs/tags/*"]
        create = group sahara-guestagent-milestone
        pushTag = group sahara-guestagent-milestone
[receive]
        requireChangeId = true
        requireContributorAgreement = true
[submit]
        mergeContent = true
